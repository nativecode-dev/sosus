{
  "name": "@sosus/api",
  "version": "0.0.0",
  "description": "SOSUS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nativecode-dev/sosus.git"
  },
  "bugs": {
    "url": "https://github.com/nativecode-dev/sosus/issues"
  },
  "homepage": "https://github.com/nativecode-dev/sosus/tree/master/executables/api#readme",
  "author": "opensource@nativecode.com",
  "keywords": [
    "SOSUS"
  ],
  "main": "bin/index.js",
  "files": [
    "bin"
  ],
  "directories": {
    "lib": "bin",
    "test": "specs"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "monitor": "nodemon",
    "upgrade": "npx npm-check -y"
  },
  "nodemonConfig": {
    "exec": "ts-node src",
    "ext": "ts",
    "ignore": [
      "dist",
      "node_modules"
    ],
    "signal": "SIGHUP",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@sosus/core": "^0.0.0",
    "@sosus/core-data": "^0.0.0",
    "@sosus/core-process": "^0.0.0",
    "@sosus/core-web": "^0.0.0",
    "@sosus/data-media": "~0.0.0",
    "@sosus/data-people": "~0.0.0",
    "@sosus/data-system": "~0.0.0",
    "express": "~4.17.1",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@types/express": "~4.17.2",
    "@types/node": "~13.1.4",
    "nodemon": "~2.0.2"
  }
}
